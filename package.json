{
  "name": "@wmfs/tymly-etl-plugin",
  "version": "0.0.0-semantically-released",
  "description": "A collection of states for helping with Extract, Transform and Load tasks.",
  "homepage": "https://github.com/wmfs/tymly-etl-plugin#readme",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "plugin",
    "extract",
    "transform",
    "load",
    "tasks"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-etl-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-etl-plugin/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "boom": "7.2.0",
    "debug": "3.1.0",
    "glob": "7.1.2",
    "@wmfs/tymly": "1.3.0",
    "@wmfs/smithereens": "1.1.1",
    "@wmfs/xml-flatten2csv": "1.0.1",
    "@wmfs/xml2csv": "1.0.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",
    "csvtojson": "2.0.4",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.2",
    "standard": "11.0.1",
    "@wmfs/tymly-test-helpers": "1.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"},
        {"type": "build", "scope":"deps-dev", "release": "patch"}
      ]
    }
  }
}
